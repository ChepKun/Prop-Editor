<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
<TableLayout android:id="@+id/TableLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
	<TableRow>
		<TextView android:id="@+id/text_prop" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Property=" />
		<TextView android:id="@+id/text_val" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Value" />
	</TableRow>
</TableLayout>
<ListView android:id="@+id/PropList" android:layout_width="wrap_content" android:layout_height="wrap_content"></ListView>
</LinearLayout>




